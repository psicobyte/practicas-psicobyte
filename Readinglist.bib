@article{DeAraujoFarias2013,
abstract = {We have investigated the capacity of activated carbon cloth to support the growth and differentiation of human mesenchymal umbilical-cord stromal stem cells. Our results demonstrate that this scaffold provides suitable conditions for the development of cell-derived matrix proteins and facilitates the growth of undifferentiated stem cells with the ability to induce osteogenic and chondrogenic differentiation. Immunoflourescence staining revealed extensive expression of collagen in all the samples, and collagen type II and osteopontin within the samples cultivated in specific differentiation-inducing media. Cell growth and the formation of natural collagen, calcium-magnesium carbonate and hydroxyapatite crystals, together with the self-assemblage of collagen to produce suprafibrillar arrangements of fibrils all occur simultaneously and can be studied together ex vivo under physiological conditions. Furthermore, the spontaneous differentiation of stem cells cultured on activated carbon cloth with no osteogenic supplements opens up new possibilities for bone-tumour engineering and treatment of traumatic and degenerative bone diseases. {\textcopyright} 2013 The Royal Society of Chemistry.},
annote = {NULL},
author = {{de Ara{\'{u}}jo Farias}, Virg{\'{i}}nea and L{\'{o}}pez-Pe{\~{n}}alver, Jes{\'{u}}s J. and Sir{\'{e}}s-Campos, Julia and L{\'{o}}pez-Ram{\'{o}}n, Mar{\'{i}}a Victoria and Moreno-Castilla, Carlos and Oliver, Francisco Javier and {Ruiz de Almod{\'{o}}var}, Jos{\'{e}} Mariano},
doi = {10.1039/c3tb20305k},
file = {:C$\backslash$:/Users/Dunja/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/de Ara{\'{u}}jo Farias et al. - 2013 - Growth and spontaneous differentiation of umbilical-cord stromal stem cells on activated carbon cloth.pdf:pdf},
issn = {2050-750X},
journal = {Journal of Materials Chemistry B},
number = {27},
pages = {3359},
title = {{Growth and spontaneous differentiation of umbilical-cord stromal stem cells on activated carbon cloth}},
url = {http://xlink.rsc.org/?DOI=c3tb20305k},
volume = {1},
